\h
GRANT ALL ON trails_forward_development TO bendyworker;
SHOW schemas
;
SHOW tables;
GRANT ALL ON public TO bendyworker;
SHOW search_path;
GRANT ALL ON bendyworker TO bendyworker;
SHOW databases;
SHOW databases
;
GRANT ALL ON SCHEMA bendyworker TO bendyworker;
GRANT ALL ON SCHEMA "$user" TO bendyworker;
GRANT ALL ON SCHEMA public TO bendyworker;
exit
\q
\h
exit
exit
\q
GRANT ALL PRIVILEGES ON public TO bendyworker;
GRANT ALL PRIVILEGES ON SCHEMA public TO bendyworker;
\q
Tribble
;
describe agents;
\d agents;
exit
;
\q
\d agents
exit
;
\q
GRANT ALL PRIVILEGES ON SCHEMA public TO bendyworker;
exit
\q
\d agents
\d agent_properties
exit
\q
