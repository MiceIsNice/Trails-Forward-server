CREATE OR REPLACE FUNCTION agents_insert_trigger()
RETURNS TRIGGER AS $$
BEGIN
  IF ( x >= 0 AND x < 200 AND y >= 0 AND y < 200 ) THEN
    INSERT INTO agents_x0_y0 VALUES (NEW.*);
  ELSIF ( x >= 0 AND x < 200 AND y >= 200 AND y < 400 ) THEN
    INSERT INTO agents_x0_y200 VALUES (NEW.*);
  ELSIF ( x >= 0 AND x < 200 AND y >= 400 AND y < 600 ) THEN
    INSERT INTO agents_x0_y400 VALUES (NEW.*);
  ELSIF ( x >= 0 AND x < 200 AND y >= 600 AND y < 800 ) THEN
    INSERT INTO agents_x0_y600 VALUES (NEW.*);
  ELSIF ( x >= 0 AND x < 200 AND y >= 800 AND y < 1000 ) THEN
    INSERT INTO agents_x0_y800 VALUES (NEW.*);
  ELSIF ( x >= 200 AND x < 400 AND y >= 0 AND y < 200 ) THEN
    INSERT INTO agents_x200_y0 VALUES (NEW.*);
  ELSIF ( x >= 200 AND x < 400 AND y >= 200 AND y < 400 ) THEN
    INSERT INTO agents_x200_y200 VALUES (NEW.*);
  ELSIF ( x >= 200 AND x < 400 AND y >= 400 AND y < 600 ) THEN
    INSERT INTO agents_x200_y400 VALUES (NEW.*);
  ELSIF ( x >= 200 AND x < 400 AND y >= 600 AND y < 800 ) THEN
    INSERT INTO agents_x200_y600 VALUES (NEW.*);
  ELSIF ( x >= 200 AND x < 400 AND y >= 800 AND y < 1000 ) THEN
    INSERT INTO agents_x200_y800 VALUES (NEW.*);
  ELSIF ( x >= 400 AND x < 600 AND y >= 0 AND y < 200 ) THEN
    INSERT INTO agents_x400_y0 VALUES (NEW.*);
  ELSIF ( x >= 400 AND x < 600 AND y >= 200 AND y < 400 ) THEN
    INSERT INTO agents_x400_y200 VALUES (NEW.*);
  ELSIF ( x >= 400 AND x < 600 AND y >= 400 AND y < 600 ) THEN
    INSERT INTO agents_x400_y400 VALUES (NEW.*);
  ELSIF ( x >= 400 AND x < 600 AND y >= 600 AND y < 800 ) THEN
    INSERT INTO agents_x400_y600 VALUES (NEW.*);
  ELSIF ( x >= 400 AND x < 600 AND y >= 800 AND y < 1000 ) THEN
    INSERT INTO agents_x400_y800 VALUES (NEW.*);
  ELSIF ( x >= 600 AND x < 800 AND y >= 0 AND y < 200 ) THEN
    INSERT INTO agents_x600_y0 VALUES (NEW.*);
  ELSIF ( x >= 600 AND x < 800 AND y >= 200 AND y < 400 ) THEN
    INSERT INTO agents_x600_y200 VALUES (NEW.*);
  ELSIF ( x >= 600 AND x < 800 AND y >= 400 AND y < 600 ) THEN
    INSERT INTO agents_x600_y400 VALUES (NEW.*);
  ELSIF ( x >= 600 AND x < 800 AND y >= 600 AND y < 800 ) THEN
    INSERT INTO agents_x600_y600 VALUES (NEW.*);
  ELSIF ( x >= 600 AND x < 800 AND y >= 800 AND y < 1000 ) THEN
    INSERT INTO agents_x600_y800 VALUES (NEW.*);
  ELSIF ( x >= 800 AND x < 1000 AND y >= 0 AND y < 200 ) THEN
    INSERT INTO agents_x800_y0 VALUES (NEW.*);
  ELSIF ( x >= 800 AND x < 1000 AND y >= 200 AND y < 400 ) THEN
    INSERT INTO agents_x800_y200 VALUES (NEW.*);
  ELSIF ( x >= 800 AND x < 1000 AND y >= 400 AND y < 600 ) THEN
    INSERT INTO agents_x800_y400 VALUES (NEW.*);
  ELSIF ( x >= 800 AND x < 1000 AND y >= 600 AND y < 800 ) THEN
    INSERT INTO agents_x800_y600 VALUES (NEW.*);
  ELSIF ( x >= 800 AND x < 1000 AND y >= 800 AND y < 1000 ) THEN
    INSERT INTO agents_x800_y800 VALUES (NEW.*);
  ELSIF ( x >= 1000 AND x < 1200 AND y >= 0 AND y < 200 ) THEN
    INSERT INTO agents_x1000_y0 VALUES (NEW.*);
  ELSIF ( x >= 1000 AND x < 1200 AND y >= 200 AND y < 400 ) THEN
    INSERT INTO agents_x1000_y200 VALUES (NEW.*);
  ELSIF ( x >= 1000 AND x < 1200 AND y >= 400 AND y < 600 ) THEN
    INSERT INTO agents_x1000_y400 VALUES (NEW.*);
  ELSIF ( x >= 1000 AND x < 1200 AND y >= 600 AND y < 800 ) THEN
    INSERT INTO agents_x1000_y600 VALUES (NEW.*);
  ELSIF ( x >= 1000 AND x < 1200 AND y >= 800 AND y < 1000 ) THEN
    INSERT INTO agents_x1000_y800 VALUES (NEW.*);
  ELSIF ( x >= 1200 AND x < 1400 AND y >= 0 AND y < 200 ) THEN
    INSERT INTO agents_x1200_y0 VALUES (NEW.*);
  ELSIF ( x >= 1200 AND x < 1400 AND y >= 200 AND y < 400 ) THEN
    INSERT INTO agents_x1200_y200 VALUES (NEW.*);
  ELSIF ( x >= 1200 AND x < 1400 AND y >= 400 AND y < 600 ) THEN
    INSERT INTO agents_x1200_y400 VALUES (NEW.*);
  ELSIF ( x >= 1200 AND x < 1400 AND y >= 600 AND y < 800 ) THEN
    INSERT INTO agents_x1200_y600 VALUES (NEW.*);
  ELSIF ( x >= 1200 AND x < 1400 AND y >= 800 AND y < 1000 ) THEN
    INSERT INTO agents_x1200_y800 VALUES (NEW.*);
  ELSIF ( x >= 1400 AND x < 1600 AND y >= 0 AND y < 200 ) THEN
    INSERT INTO agents_x1400_y0 VALUES (NEW.*);
  ELSIF ( x >= 1400 AND x < 1600 AND y >= 200 AND y < 400 ) THEN
    INSERT INTO agents_x1400_y200 VALUES (NEW.*);
  ELSIF ( x >= 1400 AND x < 1600 AND y >= 400 AND y < 600 ) THEN
    INSERT INTO agents_x1400_y400 VALUES (NEW.*);
  ELSIF ( x >= 1400 AND x < 1600 AND y >= 600 AND y < 800 ) THEN
    INSERT INTO agents_x1400_y600 VALUES (NEW.*);
  ELSIF ( x >= 1400 AND x < 1600 AND y >= 800 AND y < 1000 ) THEN
    INSERT INTO agents_x1400_y800 VALUES (NEW.*);
  ELSE
    RAISE EXCEPTION 'Date out of range.  Fix the agents_insert_trigger() function!';
  END IF;
  RETURN NULL;
END;
$$
LANGUAGE plpgsql;
exit
\q
